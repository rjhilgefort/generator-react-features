{
  "name": "generator-react-modules",
  "version": "0.0.1",
  "description": "Yeoman generator for react modules architecture",
  "homepage": "",
  "author": {
    "name": "Rob Hilgefort",
    "email": "rjhilgefort@gmail.com",
    "url": "rob.hilgefort.me"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman",
    "generator",
    "react",
    "blueprint",
    "feature",
    "module",
    "component",
    "yeoman-generator"
  ],
  "dependencies": {
    "chalk": "^1.1.3",
    "lodash": "^4.17.4",
    "ramda": "^0.23.0",
    "yeoman-generator": "^1.0.0",
    "yosay": "^1.2.1"
  },
  "devDependencies": {
    "eslint": "^3.1.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-config-xo-space": "^0.15.0",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-coveralls": "^0.1.0",
    "gulp-eslint": "^3.0.1",
    "gulp-exclude-gitignore": "^1.0.0",
    "gulp-istanbul": "^1.0.0",
    "gulp-line-ending-corrector": "^1.0.1",
    "gulp-mocha": "^3.0.1",
    "gulp-nsp": "^2.1.0",
    "gulp-plumber": "^1.0.0",
    "yeoman-assert": "^2.2.1",
    "yeoman-test": "^1.6.0"
  },
  "eslintConfig": {
    "extends": [
      "xo-space",
      "airbnb-base"
    ],
    "rules": {
      "comma-dangle": ["error", {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "never"
      }]
    },
    "env": {
      "mocha": true
    }
  },
  "repository": "rjhilgefort/generator-react-modules",
  "scripts": {
    "prepublish": "gulp prepublish",
    "test": "gulp",
    "lint": "eslint ./generators gulpfile.js",
    "lint-fix": "eslint ./generators gulpfile.js --fix"
  },
  "license": "MIT"
}
